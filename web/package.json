{
  "name": "jukebox",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "npm run build:js && npm run build:css",
    "build:js": "NODE_ENV=production node --env-file=.env ./esbuild.js",
    "build:css": "tailwindcss --input ./tailwind.css --output ../static/styles.css",
    "dev": "concurrently --kill-others \"npm run dev:js\" \"npm run dev:css\"",
    "dev:js": "NODE_ENV=development node --env-file=.env ./esbuild.js",
    "dev:css": "tailwindcss --input ./tailwind.css --output ../static/styles.css --watch",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@sentry/esbuild-plugin": "^2.22.6",
    "@sentry/react": "^8.40.0",
    "format-duration": "^3.0.2",
    "posthog-js": "^1.188.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-player": "^2.16.0",
    "react-use-websocket": "^4.11.1",
    "youtube-player": "^5.6.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/youtube-player": "^5.5.11",
    "concurrently": "^9.1.0",
    "esbuild": "^0.24.0",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.15"
  }
}
